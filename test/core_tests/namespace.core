class C
  <D <a,a>>
  <a>
where
  method f
    <C <a>>
    <((Arr a) a)>
  is
    {\ x -> x<a>}
  end
  instance
    <>
    <Int>
  where
    def f
      <C <Int>>
      <((Arr Int) Int)>
    is
      {\ x -> 2<Int>}
    end
  end
end

def eff
  <C <a>>
  <((Arr a) ((Pair a) a))>
is
  {\ x -> ((MkPair<((Arr a) ((Arr a) ((Pair a) a)))> x<a>) (f<((Arr a) a)> x<a>))}
end

def uff
  <>
  <((Arr Int) ((Pair Int) Int))>
is
  {\ x -> ((MkPair<((Arr Int) ((Arr Int) ((Pair Int) Int)))> x<Int>) (f<((Arr Int) Int)> x<Int>))}
end