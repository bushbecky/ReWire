data Maybe
     <a>
of
     Nothing <>
     Just <a>
end

data List
     <a>
of
     Empty <>
     Cons <a,(List a)>
end

newtype StateT
     <s m a>
of
     StateT <((Arr s) (m ((Pair a) s)))>
end

def f
     <a>
     <>
     <((Arr a) (Maybe a))>
is
     {\ x -> (Just<((Arr a) (Maybe a))> x<a>)}
end

def length
     <a>
     <>
     <((Arr (List a)) Int)>
is
     {\ l -> case l<(List a)> of
                          <(Empty)    |True<Bool>>
                             0<Int>
                          <(Cons x lt)|True<Bool>>
                             ((add<((Arr Int) ((Arr Int) Int))> 1<Int>)
                                                               (length<((Arr (List a)) Int)> lt<(List a)>)
                             )
                       end
     }
end
